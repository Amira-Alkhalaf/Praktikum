<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="swp_impl_acr.quizapy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QuestionListActivity"
            android:label="@string/title_activity_question_list"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".QuestionActivity"
            android:label="@string/title_activity_test"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".ComplexityScaleActivity" />
        <activity
            android:name=".TopicSelectionActivity"
            android:label="@string/title_activity_topic_selection"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".TypesOfQuestions" />
        <activity android:name=".Mode1" />
        <activity android:name=".Mode2" />
        <activity
            android:name=".OptionsActivity"
            android:label="@string/title_activity_options"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
    </application>

</manifest>